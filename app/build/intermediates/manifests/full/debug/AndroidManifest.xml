<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.newnote"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="18"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.newnote.NotepadActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.newnote.InputPageActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.newnote.SettingActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.newnote.IntroduceActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.newnote.Showpage"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>